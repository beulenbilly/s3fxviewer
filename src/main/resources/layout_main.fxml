<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.SplitPane?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.ToolBar?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>

<BorderPane minHeight="614.0" minWidth="800.0" xmlns="http://javafx.com/javafx/8.0.111"
            xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="com.umbertopalazzini.s3zilla.controller.S3ZillaController">
    <center>
        <SplitPane dividerPositions="0.75" orientation="VERTICAL" BorderPane.alignment="CENTER">
            <items>
                <AnchorPane>
                    <children>
                        <SplitPane dividerPositions="0.3" layoutX="283.0" layoutY="124.0" prefHeight="445.0"
                                   prefWidth="798.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
                                   AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                            <items>
                                <BorderPane prefHeight="200.0" prefWidth="200.0">
                                    <center>
                                        <ListView fx:id="foldersListView" BorderPane.alignment="CENTER"/>
                                    </center>
                                    <top>
                                        <HBox BorderPane.alignment="CENTER">
                                            <children>
                                                <Label fx:id="bucketLabel" prefHeight="26.0" text="%bucket.label">
                                                    <padding>
                                                        <Insets right="10.0"/>
                                                    </padding>
                                                </Label>
                                                <ComboBox fx:id="bucketComboBox" prefWidth="150.0"/>
                                            </children>
                                            <padding>
                                                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
                                            </padding>
                                        </HBox>
                                    </top>
                                </BorderPane>
                                <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="160.0" prefWidth="100.0">
                                    <children>
                                        <TableView fx:id="filesTable" layoutX="187.0" layoutY="84.0"
                                                   maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308"
                                                   tableMenuButtonVisible="true" AnchorPane.bottomAnchor="0.0"
                                                   AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
                                                   AnchorPane.topAnchor="0.0">
                                            <placeholder>
                                                <Label text="%filesTable.placeholder"/>
                                            </placeholder>
                                            <columns>
                                                <TableColumn fx:id="filesTable_name" editable="false" prefWidth="155.0"
                                                             sortable="false" text="%filesTable.name"/>
                                                <TableColumn fx:id="filesTable_lastModified" editable="false"
                                                             prefWidth="319.0" sortable="false"
                                                             text="%filesTable.lastModified"/>
                                                <TableColumn fx:id="filesTable_size" editable="false" maxWidth="2000.0"
                                                             minWidth="20.0" prefWidth="20.0" sortable="false"
                                                             text="%filesTable.size"/>
                                            </columns>
                                            <columnResizePolicy>
                                                <TableView fx:constant="CONSTRAINED_RESIZE_POLICY"/>
                                            </columnResizePolicy>
                                        </TableView>
                                    </children>
                                </AnchorPane>
                            </items>
                        </SplitPane>
                    </children>
                </AnchorPane>
                <AnchorPane>
                    <children>
                        <TableView fx:id="logTable" layoutX="130.0" layoutY="-15.0" prefHeight="121.0" prefWidth="798.0"
                                   tableMenuButtonVisible="true" AnchorPane.bottomAnchor="0.0"
                                   AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                            <placeholder>
                                <Label text="%logTable.placeholder"/>
                            </placeholder>
                            <columns>
                                <TableColumn fx:id="logTable_localFile" prefWidth="150.0" text="%logTable.localFile"/>
                                <TableColumn fx:id="logTable_remoteFile" minWidth="0.0" prefWidth="175.0"
                                             text="%logTable.remoteFile"/>
                                <TableColumn fx:id="logTable_progress" prefWidth="166.0" text="%logTable.progress"/>
                                <TableColumn fx:id="logTable_size" prefWidth="124.0" text="%logTable.size"/>
                                <TableColumn fx:id="logTable_status" prefWidth="75.0" text="%logTable.status"/>
                            </columns>
                            <columnResizePolicy>
                                <TableView fx:constant="CONSTRAINED_RESIZE_POLICY"/>
                            </columnResizePolicy>
                        </TableView>
                    </children>
                </AnchorPane>
            </items>
        </SplitPane>
    </center>
    <top>
        <ToolBar BorderPane.alignment="CENTER">
            <items>
                <Button fx:id="downloadButton" disable="true" mnemonicParsing="false" onMouseClicked="#download"
                        text="%button.download"/>
                <Button fx:id="uploadButton" mnemonicParsing="false" text="%button.upload"/>
            </items>
        </ToolBar>
    </top>
</BorderPane>
